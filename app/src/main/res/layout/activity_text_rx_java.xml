<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".test.networktest.TextRxJavaActivity">


    <Button
        android:id="@+id/btn_rxjava_login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="登录" />

    <EditText
        android:id="@+id/et_rxjava_token"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="用户token" />

    <Button
        android:id="@+id/btn_rxjava_userinfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="用户信息" />

    <Button
        android:id="@+id/btn_rxjava_update_email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="修改邮箱" />

    <Button
        android:id="@+id/btn_rxjava_register"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="注册" />

    <EditText
        android:text="对3.2的修改邮箱的代码逻辑进行修改"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>